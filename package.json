{
  "name": "de-spoil-owl",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:3000",
  "scripts": {
    "dev": "concurrently \"npm run start-client\" \"npm run start-server\"",
    "start-client": "set PORT=8080 && react-scripts start",
    "start-server": "nodemon --watch ./server ./server/app.js",
    "build": "react-scripts build",
    "pretest": "eslint ./server ./src/*.js* --fix",
    "test": "echo 'eslint passed successfully, no test written'",
    "eject": "react-scripts eject",
    "start": "node ./server/app.js"
  },
  "dependencies": {
    "async": "^2.6.2",
    "body-parser": "^1.18.3",
    "compression": "^1.7.4",
    "connect-redis": "^3.4.1",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.9.0",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "google-spreadsheet": "^2.0.7",
    "mongoose": "^5.4.22",
    "node-sass": "^4.11.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-router-dom": "^5.0.0",
    "react-scripts": "2.1.8"
  },
  "devDependencies": {
    "concurrently": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.16.0",
    "nodemon": "^1.18.10"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
